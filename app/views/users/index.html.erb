<div class="row">
    <div class="col-md-4">
	<div style="height:300; overflow-y: auto;">
	    <div class="panel panel-default">
		<div class="panel-heading">Registered Users</div>

		<table class="table">
		    <thead>
			<tr>
			    <th>#</th>
			    <th>Name</th>
			    <th></th>
			</tr>
		    </thead>
		    <tbody>
			<% @users.each_with_index do |user, index| %>
			    <tr>
				<td><%= index += 1 %></td>
				<td><%= user.name %></td>
			    </tr>
			<% end %>
		    </tbody>

		</table>
	    </div>
	</div>
    </div>
</div>
